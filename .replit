modules = ["python-3.11", "postgresql-16"]
run = "python myCWK/manage.py runserver 0.0.0.0:8000"

[nix]
channel = "stable-24_05"

[unitTest]
language = "python3"

[gitHubImport]
requiredFiles = [".replit", "replit.nix"]

[deployment]
deploymentTarget = "static"
always-on = true
publicDir = "./"

[auth]
pageEnabled = true
buttonEnabled = false

[[ports]]
localPort = 8000
externalPort = 80
